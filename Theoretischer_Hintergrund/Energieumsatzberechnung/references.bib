file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lunn, Axtell - 2021 - Validity and Reliability of the Lode Excalibur Sport Cycle Ergometer for the Wingate Anaerobic Test.pdf:pdf},
issn = {0195-9131},
journal = {Medicine & Science in Sports & Exercise},
keywords = {accuracy,cycling,power,sit,sprint,watts},
number = {5S},
