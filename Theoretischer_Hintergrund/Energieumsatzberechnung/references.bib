journal = {International Journal of Sports Medicine},
keywords = {Cadence,Fatigue,Locomotion,Oxygen uptake},
number = {1},
pages = {60--64},
pmid = {10683101},
title = {{Energetically optimal cadence vs. freely-chosen cadence during cycling: Effect of exercise duration}},
