issn = {13665847},
journal = {Ergonomics},
number = {2},
pages = {147--156},
pmid = {7398614},
title = {{Transfers of mechanical energy within the total body and mechanical efficiency during treadmill walking}},
volume = {23},
year = {1980}
}
