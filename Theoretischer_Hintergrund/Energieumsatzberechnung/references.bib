file = {:C\:/Users/johan/OneDrive/Desktop/SpoWi/WS 22,23/Masterarbeit - Wirkungsgrad/Studien, Paper/diPrampero1983_Oxygen deficit and stores at onset.pdf:pdf},
issn = {01617567},
journal = {Journal of Applied Physiology Respiratory Environmental and Exercise Physiology},
number = {1 I},
pages = {146--153},
pmid = {6885564},
title = {{Oxygen deficit and stores at onset of muscular exercise in humans}},
