file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mader - 2003 - Glycolysis and oxidative phosphorylation as a function of cytosolic phosphorylation state and power output of the muscle.pdf:pdf},
issn = {14396319},
journal = {European Journal of Applied Physiology},
keywords = {Computer simulation of energy metabolisms,Cytosolic ATP/PCr equilibrium,Mathematical model,Regulation of oxidative phosphorylation and glycol},
number = {4-5},
