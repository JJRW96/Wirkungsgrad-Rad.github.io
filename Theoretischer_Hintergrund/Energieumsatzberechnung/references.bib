issn = {1365201X},
journal = {Acta Physiologica Scandinavica},
keywords = {anaerobic threshold,bicycle ergometer,energy consumption,mechanical efficiency,mechanical work.},
number = {3},
pages = {331--337},
pmid = {3425344},
title = {{Mechanical work and efficiency in ergometer bicycling at aerobic and anaerobic thresholds}},
volume = {131},
year = {1987}
}
