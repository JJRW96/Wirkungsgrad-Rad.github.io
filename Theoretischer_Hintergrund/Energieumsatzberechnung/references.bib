issn = {15407748},
journal = {Journal of General Physiology},
number = {2},
pages = {159--174},
pmid = {312312},
title = {{The relationship between initial creatine phosphate breakdown and recovery oxygen consumption for a single isometric tetanus of the frog sartorius muscle at 20°C}},
volume = {73},
