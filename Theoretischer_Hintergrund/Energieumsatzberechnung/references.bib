doi = {10.1007/BF00422734},
file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tesch, Colliander, Kaiser - 1986 - Muscle metabolism during intense, heavy-resistance exercise.pdf:pdf},
issn = {03015548},
journal = {European Journal of Applied Physiology and Occupational Physiology},
keywords = {ATP, CP, glucose,Glucose-6-phosphate,Glycogen,Lactate,Strength training,$\alpha$-glycerophosphoate},
number = {4},
pages = {362--366},
