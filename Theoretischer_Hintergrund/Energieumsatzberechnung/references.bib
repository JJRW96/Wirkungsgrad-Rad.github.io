file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mol{\'{e}} et al. - 1999 - Myoglobin desaturation with exercise intensity in human gastrocnemius muscle.pdf:pdf},
issn = {03636119},
journal = {American Journal of Physiology - Regulatory Integrative and Comparative Physiology},
keywords = {Bioenergetics,Metabolism,Nuclear magnetic resonance,Oxygen},
number = {1 46-1},
pages = {173--180},
pmid = {10409271},
title = {{Myoglobin desaturation with exercise intensity in human gastrocnemius muscle}},
volume = {277},
year = {1999}
}
