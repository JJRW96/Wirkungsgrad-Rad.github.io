journal = {Gait and Posture},
keywords = {Energy cost of walking,Net evaluation protocol,Oxygen-uptake,Validation,VmaxST system},
number = {1},
pages = {67--73},
pmid = {15196523},
title = {{Validation of the portable VmaxST system for oxygen-uptake measurement}},
