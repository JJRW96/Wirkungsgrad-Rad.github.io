ng. {\textcopyright} 1991 by the American College of Sports Medicine.},
author = {Coyle, E. F. and Feltner, M. E. and Kautz, S. A. and Hamilton, M. T. and Montain, S. J. and Baylor, A. M. and Abraham, L. D. and Petrek, G. W.},
doi = {10.1249/00005768-199101000-00015},
file = {:C\:/Users/johan/OneDrive/Desktop/SpoWi/WS 22,23/Masterarbeit - Wirkungsgrad/Studien, Paper/Coyle1991_Physiological_and_biomechanical_factors_associated.15_ocred.pdf:pdf},
