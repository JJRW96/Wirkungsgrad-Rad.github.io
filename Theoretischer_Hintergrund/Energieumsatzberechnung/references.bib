issn = {15221601},
journal = {Journal of Applied Physiology},
keywords = {Carbon dioxide output,Incremental exercise,Lactate threshold,Oxygen uptake,Standard bicarbonate},
number = {6},
pages = {2020--2027},
title = {{A new method for detecting anaerobic threshold by gas exchange}},
volume = {121},
year = {1986}
}
