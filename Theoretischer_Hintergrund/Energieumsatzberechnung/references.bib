number = {18},
pages = {5845--5850},
pmid = {4369824},
publisher = {{\^{A}}{\textcopyright} 1974 ASBMB. Currently published by Elsevier Inc; originally published by American Society for Biochemistry and Molecular Biology.},
title = {{Calculated equilibria of phosphocreatine and adenosine phosphates during utilization of high energy phosphate by muscle}},
