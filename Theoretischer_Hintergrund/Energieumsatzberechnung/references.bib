journal = {Medicine and science in sports and exercise},
keywords = {Adipose Tissue,Adult,Bicycling,Energy Metabolism,Exercise Test,Female,Heart Rate,Humans,Male,Muscles,Oxygen Consumption,Posture,physiology},
language = {eng},
