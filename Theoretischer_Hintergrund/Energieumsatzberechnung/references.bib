journal = {International Journal of Exercise Science},
keywords = {Breathing,Cycling economy,Muscle activation,Pedaling entrainment},
number = {2},
pages = {996--1011},
pmid = {32922629},
title = {{Physiological and biomechanical differences between seated and standing uphill cycling}},
