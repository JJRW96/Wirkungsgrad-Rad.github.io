file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shiomi et al. - 2000 - Physiological Responses and Mechanical Efficiency during Different Types of Ergometric Exercise.pdf:pdf},
