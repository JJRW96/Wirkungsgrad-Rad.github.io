keywords = {Inverse dynamics,mechanical efficiency,optimization,performance},
number = {8},
pages = {907--913},
pmid = {28671851},
publisher = {Routledge},
title = {{Spontaneous change from seated to standing cycling position with increasing power is associated with a minimization of cost functions}},
url = {https://doi.org/10.1080/02640414.2017.1346272},
volume = {36},
year = {2018}
}
