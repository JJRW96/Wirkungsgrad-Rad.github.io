pmid = {36216336},
title = {{Reliability of the 3-Component Model of Aerobic, Anaerobic Lactic, and Anaerobic Alactic Energy Distribution (PCr-LA-O2) for Energetic Profiling of Continuous and Intermittent Exercise}},
volume = {17},
year = {2022}
}
