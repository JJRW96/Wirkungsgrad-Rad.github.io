journal = {Sports Medicine},
keywords = {Acidosis,Biomechanics,Blood Circulation,Body Temperature Regulation,Carbohydrates,Cardiac Output,Central Nervous System,Concentric Contraction,Cycling,Eccentric Contraction,Electric Stimulation,Electromyography,Endurance Sports,Energy Metabolism,Erythrocytes,Exercise Intensity,Exercise Physiology,Fatigue -- Classification,Fatigue -- Etiology,Fatigue -- Psychosocial Factors,Feedback,Glycogen,Lactates -- Blood,Models,Motivation,Muscle,Muscle Contraction,Muscle Fatigue,Neuromuscular Control,Oxygen Consumption,Physiological,Plasma Volume,Psychology,Skeletal -- Blood Supply,Skeletal -- Pathology,Skeletal -- Physiology,Soft Tissue Injuries,Sports,Stress,Theoretical},
number = {10},
pages = {865--898},
title = {{Models to explain fatigue during prolonged endurance cycling.}},
url = {http://ezaccess.libraries.psu.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=cin20&AN=106380506&site=ehost-live&scope=site},
