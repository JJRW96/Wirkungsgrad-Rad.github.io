issn = {00365513},
journal = {Scandinavian Journal of Clinical and Laboratory Investigation},
number = {1},
pages = {56--66},
pmid = {6031321},
title = {{Breakdown and resynthesis of phosphorylcreatine and adenosine triphosphate in connection with muscular work in man}},
