isbn = {0161756719},
issn = {01617567},
journal = {Journal of Applied Physiology},
keywords = {aerobic and anaerobic metabolism,arterial oxygen saturation,blood lactate,end-tidal oxygen and carbon dioxide pressures,lung volumes,oxygen-carbon dioxide diagram},
number = {2},
