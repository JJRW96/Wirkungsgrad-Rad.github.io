file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Leirdal, Ettema - 2011 - The relationship between cadence , pedalling technique and gross e Y ciency in cycling.pdf:pdf},
issn = {1439-6327},
journal = {European journal of applied physiology},
keywords = {Adult,Bicycling/*physiology,Efficiency/*physiology,Energy Metabolism/*physiology,Exercise Test/methods,Humans,Oxygen Consumption/physiology,Physical Exertion/*physiology,Posture/physiology,Psychomotor Performance/*physiology,Young Adult},
language = {eng},
