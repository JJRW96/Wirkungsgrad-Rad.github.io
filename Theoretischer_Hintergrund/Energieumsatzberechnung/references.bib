file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Berkemeier et al. - 2020 - Physiological and biomechanical differences between seated and standing uphill cycling.pdf:pdf},
issn = {1939795X},
