keywords = {Energy Metabolism,Ergometry,Exercise,Humans,Locomotion,Muscle,Oxidation-Reduction,Oxygen Consumption,Physical Endurance,Skeletal,metabolism,physiology},
language = {eng},
month = {jan},
number = {1},
pages = {537--562},
pmid = {23728979},
title = {{Bioenergetics of exercising humans.}},
volume = {2},
year = {2012}
