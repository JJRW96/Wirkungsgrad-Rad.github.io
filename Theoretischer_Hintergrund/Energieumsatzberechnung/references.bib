issn = {14396319},
journal = {European Journal of Applied Physiology},
keywords = {Breath-by-breath alveolar O2 transfer,End-expiratory lung volume,Gas exchange kinetics,Gas lung stores,Time constants},
number = {3},
pages = {203--213},
pmid = {12458363},
title = {{Breath-by-breath alveolar oxygen transfer at the onset of step exercise in humans: Methodological implications}},
