@article{Lucia2001,
abstract = {Professional road cycling is an extreme endurance sport. Approximately 30 000 to 35 000km are cycled each year in training and competition and some races, such as the Tour de France last 21 days (∼ 100 hours of competition) during which professional cyclists (PC) must cover >3500km. In some phases of such a demanding sport, on the other hand, exercise intensity is surprisingly high, since PC must complete prolonged periods of exercise (i.e. time trials, high mountain ascents) at high percentages (∼90%) of maximal oxygen uptake (VO2max) [above the anaerobic threshold (AT)]. Although numerous studies have analysed the physiological responses of elite, amateur level road cyclists during the last 2 decades, their findings might not be directly extrapolated to professional cycling. Several studies have recently shown that PC exhibit some remarkable physiological responses and adaptations such as: an efficient respiratory system (i.e. lack of 'tachypnoeic shift' at high exercise intensities); a considerable reliance on fat metabolism even at high power outputs; or several neuromuscular adaptations (i.e. a great resistance to fatigue of slow motor units). This article extensively reviews the different responses and adaptations (cardiopulmonary system, metabolism, neuromuscular factors or endocrine system) to this sport. A special emphasis is placed on the evaluation of performance both in the laboratory (i.e. the controversial Conconi test, distinction between climbing and time trial ability, etc.) and during actual competitions such as the Tour de France.},
author = {Lucia, A. and Hoyos, J. and Chicharro, J. L.},
doi = {10.2165/00007256-200131050-00004},
file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lucia, Hoyos, Chicharro - 2001 - Physiology of professional road cycling.pdf:pdf},
issn = {01121642},
journal = {Sports Medicine},
number = {5},
pages = {325--337},
pmid = {11347684},
title = {{Physiology of professional road cycling}},
volume = {31},
year = {2001}
}
@article{Lucia2001a,
abstract = {Purpose: The aim of this investigation was to evaluate the preferred cycling cadence of professional riders during competition. Methods: We measured the cadence of seven professional cyclists (28 ± 1 yr) during 3-wk road races (Giro d'Italia, Tour de France, and Vuelta a Espafĩa) involving three main competition requirements: uphill cycling (high mountain passes of ∼15 km, or HM); individual time trials of ∼50 km on level ground (TT); and flat, long (∼190 km) group stages (F). Heart rate (HR) data were also recorded as an indicator of exercise intensity during HM, TT, and F. Results: Mean cadence was significantly lower (P < 0.01) during HM (71.0 ± 1.4 rpm) than either F and TT (89.3 ± 1.0 and 92.4 ± 1.3 rpm, respectively). HR was similar during HM and TT (157 ± 4 and 158 ± 3 bpm) and in both cases higher (P < 0.01) than during F (124 ± 2 bpm). Conclusion: During both F and TT, professional riders spontaneously adopt higher cadences (around 90 rpm) than those previously reported in the majority of laboratory studies as being the most economical. In contrast, during HM they seem to adopt a more economical pedalling rate (∼70 rpm), possibly as a result of the specific demands of this competition phase.},
author = {Luc{\'{i}}a, A. and Hoyos, J. and Chicharro, J. L.},
doi = {10.1097/00005768-200108000-00018},
file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Luc{\'{i}}a, Hoyos, Chicharro - 2001 - Preferred pedalling cadence in professional cycling.pdf:pdf},
issn = {01959131},
journal = {Medicine and Science in Sports and Exercise},
keywords = {Cyclists,Heart rate,Pedalling frequency},
number = {8},
pages = {1361--1366},
pmid = {11474339},
title = {{Preferred pedalling cadence in professional cycling}},
volume = {33},
year = {2001}
}
@article{Luhtanen1987,
abstract = {Internal and external mechanical work, energy consumption and mechanical efficiency were studied in constant‐load ergometer bicycling at five different power outputs below, equal to, and above the aerobic (AerT) and anaerobic (AnT) thresholds. The gross, net and true efficiencies of the whole body in five male subjects were calculated. The work against the external load was defined as the external mechanical work. The internal mechanical work was calculated as the sum of the increments of kinetic and potential energy in all body segments by using methods of film analysis. Total energy consumption was measured by combining aerobic and anaerobic energy production. When the power output of the bicycle ergometer was increased from 146+15 to 283 ± 17 W, oxygen consumption increased from 2.20 ± 0.98 to 4.22 ± 0.201 min‐1 (P < 0.001), while the oxygen consumption at rest was 0.30 ± 0.03 l min‐1. The concentration of blood lactate increased from 2.2 ± 0.4 at the lowest work load to 8.6 ± 1.2 mmol 1‐1 at the highest work load (P < 0.001). The amount of external work done per revolution increased from 139 ± 20 to 277 ± 29 J (P < 0.001), while the amount of internal work per revolution remained almost constant (56 ± 12 J). The gross efficiency in the present study was 17–20%, net efficiency 18–22% and true efficiency 21–30%, respectively. The highest gross and net efficiencies were reached at the AerT. The lowest efficiencies were obtained at highest work load. Oxygen uptake and total energy consumption were significantly higher and true efficiency lower at the AnT than AerT (P < 0.01). {\textcopyright} 1987 Scandinavian Physiological Society},
author = {Luhtanen, P. and Rahkila, P. and Rusko, H. and Viitasalo, J. T.},
doi = {10.1111/j.1748-1716.1987.tb08247.x},
file = {:C\:/Users/johan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Luhtanen et al. - 1987 - Mechanical work and efficiency in ergometer bicycling at aerobic and anaerobic thresholds.pdf:pdf},
issn = {1365201X},
journal = {Acta Physiologica Scandinavica},
keywords = {anaerobic threshold,bicycle ergometer,energy consumption,mechanical efficiency,mechanical work.},
number = {3},
pages = {331--337},
pmid = {3425344},
title = {{Mechanical work and efficiency in ergometer bicycling at aerobic and anaerobic thresholds}},
volume = {131},
year = {1987}
